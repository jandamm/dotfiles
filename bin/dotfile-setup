#!/usr/bin/env bash
# vim:set foldmethod=marker:

if [ -z "$DOTFILES" ]
then
	echo "Not yet set up. Sourcing zshenv"
	# shellcheck source=../zshenv
	source "$(pwd)/${0%/*}/../zshenv"
fi

while [[ -n "$1" ]]
do
	echo $1
	
done
echo "Not yet impletemented"
exit 1


# install Brewfile
brew bundle

# set env (DOTFILES)

